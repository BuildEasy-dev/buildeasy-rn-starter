{
  "name": "buildeasy-rn-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build:preview": "eas build --profile preview --platform all",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:prod": "eas build --profile production --platform all",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:local:preview": "eas build --profile preview --platform all --local",
    "build:local:preview:ios": "eas build --profile preview --platform ios --local",
    "build:local:preview:android": "eas build --profile preview --platform android --local",
    "build:local:prod": "eas build --profile production --platform all --local",
    "build:local:prod:ios": "eas build --profile production --platform ios --local",
    "build:local:prod:android": "eas build --profile production --platform android --local",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "prepare": "husky",
    "commitlint:check": "commitlint --from HEAD~1 --to HEAD --verbose",
    "commitlint:last": "commitlint --from HEAD~10 --to HEAD",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "validate:photo-urls": "node scripts/validate-photo-urls.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "1.7.6",
    "@tamagui/animations-react-native": "^1.132.16",
    "@tamagui/config": "^1.132.16",
    "@tamagui/core": "^1.132.16",
    "@tamagui/font-inter": "^1.132.16",
    "@tamagui/stacks": "^1.132.16",
    "expo": "53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.6",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.18.2",
    "react-native-mmkv": "^3.3.0",
    "react-native-pager-view": "6.7.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-tab-view": "^4.1.3",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tamagui/babel-plugin": "^1.132.16",
    "@testing-library/react-native": "^13.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "babel-jest": "~29.7.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
